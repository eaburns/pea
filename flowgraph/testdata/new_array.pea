Func zero() int { return: 0 }

Func new(n int) [T] : { zero()T } {
	return: new(n, T :: zero())
}

func for:to:do:(start int, end int, f (int){}) {
	if: start <= end true: {
		f(start),
		for: start + 1 to: end do: f
	}
}

Func <=(a, b T) [false?, true?] : { <=>(T, T)[less?, equal?, greater?] } {
	return: a <=> b greater? { [false?, true?] :: [false?] } _? { [true?] }
}

