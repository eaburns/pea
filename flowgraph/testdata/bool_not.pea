// false
// true
func main() {
	print(string(!true)),
	print("\n"),
	print(string(!false))
}

const true := [false?, true?] :: [true?]
const false := [false?, true?] :: [false?]

func !(b [false?, true?]) [false?, true?] {
	return: b true? { false } false? { true }
}

func string(b [false?, true?]) string {
	return: b true? { "true" } false? { "false" }
}