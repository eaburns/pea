// 0123456789
func main() {
	for: 0 to: 9 do: (i int) {
		print_int(i)
	}
}

func for:to:do:(start int, end int, f (int){}) {
	start <= end true? {
		f(start),
		for: start + 1 to: end do: f
	}
}

Func <=(a, b T) bool : <=>(T, T)ordering {
	return: a <=> b greater? { bool :: [false?] } _? { [true?] }
}

func print_int(_ int)