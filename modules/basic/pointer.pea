Type T pointer &T

Func &(t &T) T pointer {
	return: (T pointer :: t)
}

Func *(t T pointer) &T {
	return: (&T :: t)
}

Func +=(p printer, t T pointer) printer : +=(printer, T) printer {
	return: p += (T :: (&T :: t))
}

test print_pointer {
	assert: print_string(&1) equals: "1",
	assert: print_string(&"hello") equals: "hello",
}
